  <ExecDotNet program="${tool-generateORM}" commandline="-do:dataset -petraxml:${PetraXML.file} -input:${dir.root}/csharp/ICT/Petra/Shared/lib/MHospitality/data/TypedDataSets.xml -outputNamespace:Ict.Petra.Shared.MHospitality.Data -outputdir:${dir.root}/csharp/ICT/Petra/Shared/lib/data -outputFilename:Hospitality.DataSets -StartTableId:5300 -TemplateDir:${dir.root}/csharp/ICT/PetraTools/Templates"/>
  <ExecDotNet program="${tool-generateORM}" commandline="-do:dataset -petraxml:${PetraXML.file} -input:${dir.root}/csharp/ICT/Petra/Shared/lib/MFinance/data/Finance.AP.TypedDataSets.xml -outputNamespace:Ict.Petra.Shared.MFinance.AP.Data -outputdir:${dir.root}/csharp/ICT/Petra/Shared/lib/data -outputFilename:Finance.AP.DataSets -StartTableId:5400 -TemplateDir:${dir.root}/csharp/ICT/PetraTools/Templates"/>
  <ExecDotNet program="${tool-generateORM}" commandline="-do:dataset -petraxml:${PetraXML.file} -input:${dir.root}/csharp/ICT/Petra/Shared/lib/MFinance/data/Finance.GL.TypedDataSets.xml -outputNamespace:Ict.Petra.Shared.MFinance.GL.Data -outputdir:${dir.root}/csharp/ICT/Petra/Shared/lib/data -outputFilename:Finance.GL.DataSets -StartTableId:5500 -TemplateDir:${dir.root}/csharp/ICT/PetraTools/Templates"/>
  <ExecDotNet program="${tool-generateORM}" commandline="-do:dataset -petraxml:${PetraXML.file} -input:${dir.root}/csharp/ICT/Petra/Shared/lib/MFinance/data/Finance.Gift.TypedDataSets.xml -outputNamespace:Ict.Petra.Shared.MFinance.Gift.Data -outputdir:${dir.root}/csharp/ICT/Petra/Shared/lib/data -outputFilename:Finance.Gift.DataSets -StartTableId:5600 -TemplateDir:${dir.root}/csharp/ICT/PetraTools/Templates"/>
  <ExecDotNet program="${tool-generateORM}" commandline="-do:dataset -petraxml:${PetraXML.file} -input:${dir.root}/csharp/ICT/Petra/Shared/lib/MSysMan/data/SysMan.TypedDataSets.xml -outputNamespace:Ict.Petra.Shared.MSysMan.Data -outputdir:${dir.root}/csharp/ICT/Petra/Shared/lib/data -outputFilename:SysMan.DataSets -StartTableId:5700 -TemplateDir:${dir.root}/csharp/ICT/PetraTools/Templates"/>
