<?xml version="1.0" encoding="utf-8" ?>
<configuration>
<system.runtime.remoting>
    <application>
        <channels>
            <channel ref="http" />
            <serverProviders>
                <formatter href="binary" />
            </serverProviders>
        </channels>
    </application>
</system.runtime.remoting>
<appSettings>
    <add key="Server.DebugLevel" value="0" />                        <!-- Default Value: "0" -->
    <add key="Server.LogFile" value="../log/WebServer.log" />   <!-- Default Value: "Server.log" in current working directory -->
    <add key="Server.RDBMSType" value="MySQL" />
    <add key="Server.PostgreSQLServer" value = "127.0.0.1" />                <!-- Default Value: "Dns.GetHostName()" -->
    <add key="Server.PostgreSQLServerPort" value = "5432" />                                <!-- Default Value: "5432" -->
    <add key="Server.PostgreSQLDatabaseName" value = "${DBMS.DBName}"/>
    <add key="Server.PostgreSQLUserName" value = "${DBMS.UserName}"/>
    <add key="Server.Credentials" value="${DBMS.Password}" />     <!-- this is the password for user petraserver for the PostgreSQL databases -->

    <add key="Reporting.PathStandardReports" value="../XmlReports" />
    <add key="Reporting.PathCustomReports" value="../XmlReports/custom" />

</appSettings>
<system.web>
    <customErrors mode="Off"/>
    <httpModules>
        <add name="DirectRequestModule" type="Ext.Net.DirectRequestModule, Ext.Net"/>
    </httpModules>
    <!-- compilation tempDirectory="/var/tmp" debug="true" -->
</system.web>

</configuration>