<configuration>
  <appSettings>
    <add key="AutoLogin" value="demo" />
    <add key="AutoLoginPasswd" value="demo" />
    <add key="Server.IPBasePort" value="-1" />                        <!-- There is no remoting enabled for testing the server -->
    <add key="Server.DebugLevel" value="0" />                        <!-- Default Value: "0" -->
    <add key="Server.LogFile" value="${dir.log}/TestServer.log" />   <!-- Default Value: "Server.log" in current working directory -->
    <add key="Server.RDBMSType" value="Postgresql" />
    <add key="Server.DBHostOrFile" value = "127.0.0.1" />                <!-- Default Value: "Dns.GetHostName()" -->
    <add key="Server.DBPort" value = "5432" />                                <!-- Default Value: "5432" -->
    <add key="Server.DBName" value = "${DBMS.DBName}"/>
    <add key="Server.DBUserName" value = "${DBMS.UserName}"/>
    <add key="Server.DBPassword" value="${DBMS.Password}" />     <!-- this is the password for user DBUserName for the PostgreSQL databases -->

    <add key="Reporting.PathStandardReports" value="${dir.root}/XmlReports" />
    <add key="Reporting.PathCustomReports" value="${dir.root}/XmlReports/custom" />
    <add key="SqlFiles.Path" value="${dir.csharp}/ICT/Petra/Server/sql" />
   </appSettings>
</configuration>